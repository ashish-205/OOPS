#include<iostream>
#include<string>
using namespace std;
int main()
{
		int a=20;
		int b=10;
		float c=20.0;
		string d="hello";
		string e="world ";
		cout<<"Sum of two integers : "<<a+b<<endl;
		cout<<"Sum of intger and float :"<<a+c<<endl;
		cout<<"Sum of string and integer :"<<a+d<<endl;
		cout<<"Sum of two strings : "<<d+e<<endl;
		
		return 0;
	}
