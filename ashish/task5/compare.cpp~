#include<iostream>
#include<string>
using namespace std;
int main()
{
	     int a=10;
	     int b=20;
	     float c=30.0;
	     string d="graphic";
	      
	      cout<<"Sum of integers : "<<a+b<<endl;
	      cout<<"Sum of integer and float : "<<a+c<<endl;
	      cout<<"Sum of integer and string : "<<a+d<<endl;
			return 0;
		}
